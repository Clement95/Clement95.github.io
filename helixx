                    <div id="slider"></div>
                </div>
                        <script>
                $( function() {
                    function calc($val) {
                        $('.invest_calc').text($val);


                        $in_day = $('.type_cript > button.active').attr('in_day');

                        $price = $('.type_cript > button.active').attr('price');
                        $name = $('.type_cript > button.active').attr('name');
                        $hash_text = $('.type_cript > button.active').attr('hash');
                        $curs = $('.type_cript > button.active').attr('curs');
                        $year = $('.type_cript > button.active').attr('year');

                        $('.years_calc').text($year+" Years");

                        $hash =  $val / $price;
                        $('.hashrate_calc').text($hash.toFixed(2) + ' '+$hash_text);
                        $earn = $hash * $in_day * 30 * $curs;
                        $('.shus_cript_calc').text($name);
                        $('.invest_calc2').text($earn.toFixed(2));
                    }
                    $( "#slider" ).slider({
                        value:0,
                        min: 0,
                        max: 10000,
                        step: 10,
                        slide: function( event, ui ) {
                            calc(ui.value);
                        }
                    });
                   $('#slider').draggable();

                    $('.type_cript > button').click(function (e) {
                        e.preventDefault();
                        $('.type_cript > button').removeClass('active');
                        $(this).addClass('active');
                        var val = $('#slider').slider("option", "value");
                        calc(val);
                    });
                } );
            </script>
